<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="mx.model.Correos" table="correos" schema="dbo" catalog="gastos" optimistic-lock="version">
        <id name="id" type="int">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="strcvevend" type="string">
            <column name="STRCVEVEND" length="5" />
        </property>
        <property name="correoAsociado" type="string">
            <column name="CORREO_ASOCIADO" length="50" />
        </property>
    </class>
</hibernate-mapping>
