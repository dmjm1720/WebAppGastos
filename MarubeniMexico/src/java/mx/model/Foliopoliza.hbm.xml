<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="mx.model.Foliopoliza" table="foliopoliza" schema="dbo" catalog="gastos" optimistic-lock="version">
        <id name="idpol" type="int">
            <column name="IDPOL" />
            <generator class="native" />
        </id>
        <property name="fechaPoliza" type="timestamp">
            <column name="FECHA_POLIZA" length="23" />
        </property>
        <property name="usuario" type="string">
            <column name="USUARIO" length="150" />
        </property>
    </class>
</hibernate-mapping>
