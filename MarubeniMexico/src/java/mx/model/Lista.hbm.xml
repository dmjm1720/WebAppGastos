<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="mx.model.Lista" table="lista" schema="dbo" catalog="gastos" optimistic-lock="version">
        <id name="conceptos" type="string">
            <column name="Conceptos" length="50" />
            <generator class="assigned" />
        </id>
        <property name="idConcepto" type="java.lang.Integer">
            <column name="idConcepto" />
        </property>
        <property name="descripcion" type="string">
            <column name="Descripcion" length="150" />
        </property>
    </class>
</hibernate-mapping>
