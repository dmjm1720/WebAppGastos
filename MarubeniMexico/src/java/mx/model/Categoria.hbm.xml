<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="mx.model.Categoria" table="categoria" schema="dbo" catalog="gastos" optimistic-lock="version">
        <id name="idCategoria" type="int">
            <column name="idCategoria" />
            <generator class="native" />
        </id>
        <property name="subDepartamento" type="string">
            <column name="subDepartamento" length="50" />
        </property>
        <property name="usuario" type="string">
            <column name="usuario" length="50" />
        </property>
    </class>
</hibernate-mapping>
