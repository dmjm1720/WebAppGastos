<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="mx.model.Catalogo" table="catalogo" schema="dbo" catalog="gastos" optimistic-lock="version">
        <id name="id" type="int">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="subcuenta" type="string">
            <column name="SUBCUENTA" length="50" />
        </property>
        <property name="cliente" type="string">
            <column name="CLIENTE" length="250" />
        </property>
    </class>
</hibernate-mapping>
